# Environment Variables for Restaurant Data Pipeline
# Copy this file to .env and fill in your actual API keys

# Yelp API Configuration
YELP_API_KEY=your_yelp_api_key_here
YELP_CLIENT_ID=your_yelp_client_id_here

# Foursquare API Configuration
FOURSQUARE_CLIENT_ID=your_foursquare_client_id_here
FOURSQUARE_CLIENT_SECRET=your_foursquare_client_secret_here

# OpenStreetMap OAuth Configuration
OPENSTREETMAP_CLIENT_ID=your_openstreetmap_client_id_here
OPENSTREETMAP_CLIENT_SECRET=your_openstreetmap_client_secret_here

# Supabase Database Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_KEY=your_supabase_anon_key_here
# Note: Use service role key for server-side operations
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# Database Configuration (if using)
DATABASE_URL=your_database_connection_string_here

# Other API Keys
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
TWILIO_API_KEY=your_twilio_api_key_here